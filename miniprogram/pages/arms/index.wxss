/**index.wxss**/
.tab-class {
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  bottom: 0;
}

.tab-body {
  width: 100%;
  height: 100%;
  background-color: aliceblue;
  padding: 10rpx;
}

.title-row {
  display: flex;
  height: 40rpx;
  line-height: 40rpx;
}

.title {
  margin-right: 20rpx;
}


.title-image {
  width: 40rpx;
  height: 40rpx;
  margin-left: 5rpx;
}